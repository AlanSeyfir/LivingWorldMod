{
  "profiles": {
    "Terraria": {
      "commandName": "Executable",
      "executablePath": "$(DotNetPath)",
      "commandLineArgs": "$(tMLPath)",
      "workingDirectory": "$(tMLSteamPath)"
    },
    "Terraria (Skip Select)": {
      "commandName": "Executable",
      "executablePath": "$(DotNetPath)",
      "commandLineArgs": "$(tMLPath) -skipselect",
